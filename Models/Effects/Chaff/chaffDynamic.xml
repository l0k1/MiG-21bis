<?xml version="1.0"?>
<PropertyList>
	<particlesystem>
		<name>chaff</name>
		<texture>puff.png</texture>
		<emissive>false</emissive>
		<lighting>false</lighting>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
			<roll-deg>0</roll-deg>
		</offsets>
		<attach>world</attach>
		<placer>
			<type>point</type>
		</placer>
		<shooter>
			<theta-min-deg>-90</theta-min-deg>
			<theta-max-deg>-90</theta-max-deg>
			<phi-min-deg>0</phi-min-deg>
			<phi-max-deg>0</phi-max-deg>
			<speed>
				<value>0</value>
				<spread>0</spread>
			</speed>
			<rotation-speed>
				<x-min-deg-sec>0</x-min-deg-sec>
				<y-min-deg-sec>0</y-min-deg-sec>
				<z-min-deg-sec>-60</z-min-deg-sec>
				<x-max-deg-sec>0</x-max-deg-sec>
				<y-max-deg-sec>0</y-max-deg-sec>
				<z-max-deg-sec>60</z-max-deg-sec>
			</rotation-speed>
		</shooter>
		<counter>
			<particles-per-sec>
				<value>25</value>
				<spread>2</spread>
			</particles-per-sec>
		</counter>
		<align>billboard</align>
		<particle>
			<start>
				<color>
					<red>
						<value>0.6</value>
					</red>
					<green>
						<value>0.6</value>
					</green>
					<blue>
						<value>0.6</value>
					</blue>
					<alpha>
						<value>0.2</value>
					</alpha>
				</color>
				<size>
					<value>1.0</value>
				</size>
			</start>
			<end>
				<color>
					<red>
						<value>0.6</value>
					</red>
					<green>
						<value>0.6</value>
					</green>
					<blue>
						<value>0.6</value>
					</blue>
					<alpha>
						<value>0</value>
					</alpha>
				</color>
				<size>
					<value>15.0</value>
				</size>
			</end>
			<life-sec>
				<value>3.5</value>
			</life-sec>
			<mass-kg>1.8</mass-kg>
			<radius-m>0.1</radius-m>
		</particle>
		<program>
			<fluid>air</fluid>
			<gravity type="bool">false</gravity>
			<wind type="bool">true</wind>
		</program>
	</particlesystem>
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>800</max-m>
	</animation>
</PropertyList>

